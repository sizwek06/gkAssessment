						"exec": [
							"var myBreed = \"shihtzu\"\r",
							"var myDoges = {\r",
							"    \"doges\":[\"basenji\",\"doberman\",\"labrador\"\r",
							"    ]\r",
							"}\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Body has my breed\", function () {\r",
							"    pm.expect(pm.response.text()).to.include(myBreed);\r",
							"});\r",
							"\r",
							"pm.test(\"Body has breeds\", function () {\r",
							"    for (var i = 0; i < myDoges.doges.length; i++) {\r",
							"    pm.expect(pm.response.text()).to.include(myDoges.doges[i]);\r",
							"    }\r",
							"});\r",
